generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CardDetails {
  id              String @id @default(cuid())
  cardholderName  String
  cardNumber      String
  cvc             String
  expirationMonth Int
  expirationYear  Int
}
