generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CardDetails {
  id              String @id @default(cuid())
  cardholderName  String
  cardNumber      BigInt
  expirationMonth Int
  expirationYear  Int
  CVC             Int
}
